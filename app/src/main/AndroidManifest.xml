<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.navigationdrawer">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NbaActivity"
            android:launchMode="singleInstance">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.admin.navigationdrawer.MainActivity" />
        </activity>
        <activity android:name=".CalculatorActivity"
            android:launchMode="singleInstance">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.admin.navigationdrawer.MainActivity" />
        </activity>
        <activity android:name=".PhotoActivity"
            android:launchMode="singleInstance">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.admin.navigationdrawer.MainActivity" />
        </activity>
        <activity android:name=".GoogleActivity"
            android:launchMode="singleInstance">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.admin.navigationdrawer.MainActivity" />
        </activity>
        <activity android:name=".DepotActivity"
            android:launchMode="singleInstance">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.admin.navigationdrawer.MainActivity" />
        </activity>
        <activity android:name=".StaplesActivity"
            android:launchMode="singleInstance">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.admin.navigationdrawer.MainActivity" />
        </activity>
        <activity android:name=".WalmartActivity"
            android:launchMode="singleInstance">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.admin.navigationdrawer.MainActivity" />
        </activity>
        <activity
            android:name=".ThinkActivity"
            android:launchMode="singleInstance" >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.admin.navigationdrawer.MainActivity" />
        </activity>
    </application>

</manifest>